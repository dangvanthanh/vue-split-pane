{
  "name": "vue-split-pane",
  "version": "0.0.9",
  "description": "Adjustable split pane using Vue.js",
  "author": "Dang Van Thanh <dangvanthanh@dangthanh.org>",
  "main": "dist/vue-split-pane.umd.js",
  "module": "dist/vue-split-pane.esm.js",
  "unpkg": "dist/vue-split-pane.min.js",
  "style": "vue-split-pane/dist/vue-split-pane.esm.css",
  "scripts": {
    "build": "npm run build:browsers && npm run build:es && npm run build:umd",
    "build:browsers": "rollup -c build/rollup.config.browsers.js",
    "build:es": "rollup -c build/rollup.config.es.js",
    "build:umd": "rollup -c build/rollup.config.umd.js",
    "test": "jest"
  },
  "peerDependencies": {
    "vue": ">= 2"
  },
  "repository": {
    "type": "git",
    "url": "vue-split-pane"
  },
  "keywords": [
    "vue",
    "split-pane",
    "vue-split-pane"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-env": "^7.11.5",
    "@dangvanthanh/prettier-config": "^1.0.0",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@testing-library/dom": "^8.2.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/vue": "^5.1.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^27.1.0",
    "clean-css": "^5.1.5",
    "esbuild": "^0.12.24",
    "jest": "^27.1.0",
    "rollup": "^2.28.2",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-esbuild": "^4.5.0",
    "rollup-plugin-vue": "^5.1.9",
    "vue-jest": "^3.0.7",
    "vue-template-compiler": "^2.6.12"
  },
  "prettier": "@dangvanthanh/prettier-config"
}
